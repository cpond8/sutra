;; macro_string_ops.sutra - Canonical macro string operation tests

;; From test_macro_concat.sutra
(define (greet name)
  (print (concat "Hello, " name "!")))
(greet "Alice")

;; From test_macro_fixed.sutra
(define (greet name) (print "Hello, Sutra!"))
(greet "unused")

;; From test_macro_native.sutra
(define (greet name) (print (+ "Hello, " name "!")))
(greet "Sutra")