;; comparison.sutra
;; Test comparison operations: eq?, gt?, lt?, gte?, lte?

;; Equality tests
(print (eq? 5 5))
(print (eq? 3 4))
(print (eq? -1 -1))
(print (eq? 0 0))

;; Greater than tests
(print (gt? 10 5))
(print (gt? 3 7))
(print (gt? 5 5))
(print (gt? -2 -5))

;; Less than tests
(print (lt? 2 8))
(print (lt? 9 4))
(print (lt? 3 3))
(print (lt? -5 -2))

;; Greater than or equal tests
(print (gte? 5 5))
(print (gte? 8 3))
(print (gte? 3 6))
(print (gte? 0 0))

;; Less than or equal tests
(print (lte? 4 4))
(print (lte? 2 7))
(print (lte? 8 2))
(print (lte? -1 -1))
