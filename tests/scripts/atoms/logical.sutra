;; logical.sutra
;; Test logical operations: not

;; Basic not operations
(print (not true))
(print (not false))

;; Nested not operations
(print (not (not true)))
(print (not (not false)))

;; Not with comparison results
(print (not (eq? 1 2)))
(print (not (eq? 3 3)))
(print (not (gt? 5 10)))
(print (not (lt? 8 3)))
