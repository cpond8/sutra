;; Simple calculator example
;; Demonstrates basic arithmetic operations

(print "=== Simple Calculator ===")
(print (+ 10 5))
(print (- 20 8))
(print (* 4 7))
(print (/ 36 6))
(print (mod 17 5))

(print "=== Comparisons ===")
(print (gt? 10 5))
(print (lt? 3 8))
(print (eq? 4 4))

(print "=== Logic ===")
(print (not true))
(print (not false))
