; Test case for path traversal vulnerability
(core/get a.b.c)      ; Valid path
(core/get a.b.c.d.e.f) ; Deeper valid path

; Invalid paths that should be rejected by the parser
(core/get a..b)       ; Empty component
(core/get ..a)        ; Leading empty component
(core/get a..)        ; Trailing empty component
(core/get a.b..c)     ; Middle empty component
(core/get a.b.c..)    ; Trailing empty component with valid path

; Path traversal attempts
(core/get a/../b)     ; Basic traversal
(core/get ../a)       ; Traversal at start
(core/get a/b/..)     ; Traversal at end